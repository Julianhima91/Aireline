{
  "version": 2,
  "crons": [
    {
      "path": "/api/route-tracking/high-demand",
      "schedule": "0 * * * *"
    },
    {
      "path": "/api/route-tracking/medium-demand", 
      "schedule": "0 */6 * * *"
    },
    {
      "path": "/api/route-tracking/low-demand",
      "schedule": "0 */12 * * *"
    }
  ],
  "env": {
    "SUPABASE_URL": "@supabase_url",
    "SUPABASE_ANON_KEY": "@supabase_anon_key",
    "SUPABASE_SERVICE_ROLE_KEY": "@supabase_service_role_key"
  }
}